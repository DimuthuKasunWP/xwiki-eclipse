From 11161c895a21e37130d1cad77eac04f263ddb99d Mon Sep 17 00:00:00 2001
From: junhan <jun.han37@gmail.com>
Date: Mon, 16 May 2011 00:20:59 -0400
Subject: [PATCH 02/13] [xeclipse_150] add parent folder and a pom.xml file to
 contain the specific platform configuration

---
 parent/pom.xml                              |   68 +++++++++++++++++++++++++++
 plugins/org.xwiki.eclipse.xmlrpc/no-pom.xml |    7 ---
 2 files changed, 68 insertions(+), 7 deletions(-)
 create mode 100644 parent/pom.xml
 delete mode 100644 plugins/org.xwiki.eclipse.xmlrpc/no-pom.xml

diff --git a/parent/pom.xml b/parent/pom.xml
new file mode 100644
index 0000000..de77acb
--- /dev/null
+++ b/parent/pom.xml
@@ -0,0 +1,68 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<project
+  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
+  xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
+  <modelVersion>4.0.0</modelVersion>
+  <groupId>org.xwiki.eclipse</groupId>
+  <artifactId>parent</artifactId>
+  <version>1.2.0.RC1</version>
+  <packaging>pom</packaging>
+
+  <!-- tycho requires maven >= 3.0 -->
+  <prerequisites>
+    <maven>3.0</maven>
+  </prerequisites>
+
+  <properties>
+    <tycho-version>0.11.1</tycho-version>
+  </properties>
+  <repositories>
+    <!-- configure p2 repository to resolve against -->
+    <repository>
+      <id>helios</id>
+      <layout>p2</layout>
+      <url>http://download.eclipse.org/releases/helios</url>
+            <!-- file URL for faster and offline builds -->
+      <!-- <url>file:/${basedir}/../../helios-rcp/</url> -->
+    </repository>
+    <!-- SWTbot is not part of the helios update site -->
+    <repository>
+      <id>swtbot</id>
+      <layout>p2</layout>
+      <url>http://download.eclipse.org/technology/swtbot/helios/dev-build/update-site/</url>
+    </repository>
+  </repositories>
+  <build>
+    <plugins>
+      <plugin>
+        <!-- enable tycho build extension -->
+        <groupId>org.sonatype.tycho</groupId>
+        <artifactId>tycho-maven-plugin</artifactId>
+        <version>${tycho-version}</version>
+        <extensions>true</extensions>
+      </plugin>
+      <plugin>
+        <groupId>org.sonatype.tycho</groupId>
+        <artifactId>target-platform-configuration</artifactId>
+        <version>${tycho-version}</version>
+        <configuration>
+          <!-- recommended: use p2-based target platform resolver -->
+          <resolver>p2</resolver>
+          <!-- configure the p2 target environments for multi-platform build -->
+          <environments>
+            <environment>
+              <os>linux</os>
+              <ws>gtk</ws>
+              <arch>x86</arch>
+            </environment>
+            <environment>
+              <os>win32</os>
+              <ws>win32</ws>
+              <arch>x86</arch>
+            </environment>
+          </environments>
+        </configuration>
+      </plugin>
+    </plugins>
+  </build>
+</project>
\ No newline at end of file
